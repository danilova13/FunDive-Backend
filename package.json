{
  "name": "fun-dive",
  "version": "1.0.0",
  "engines": {
    "node": "18.x"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --poll --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "migrate": "ts-node-dev src/db/runMigrations.ts up",
    "migrate-revert": "ts-node-dev src/db/runMigrations.ts down",
    "migrate-create": "node-pg-migrate create --tsconfig tsconfig.json -m src/db/migrations"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danilova13/FunDive.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danilova13/FunDive/issues"
  },
  "homepage": "https://github.com/danilova13/FunDive#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "apollo-server-core": "^3.12.1",
    "apollo-server-express": "^3.12.1",
    "bcrypt": "^5.1.1",
    "change-case": "^4.1.2",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-middleware": "6.1.35",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.10.49",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "ts-node": "^10.9.1"
  }
}
